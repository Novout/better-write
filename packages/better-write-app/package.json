    "vite-plugin-pages": "0.26.0",
    "vite-plugin-pages-sitemap": "1.3.0",
    "vite-plugin-pwa": "0.12.8",
    "vite-plugin-windicss": "1.8.8",
    "vue-tsc": "0.40.13"
  },
  "vite": {
    "optimizeDeps": {
      "include": [
        "@headlessui/vue",
        "@sentry/tracing",
        "@sentry/vue",
        "@supabase/supabase-js",
        "@vue/reactivity",
