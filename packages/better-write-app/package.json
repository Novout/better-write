    "better-write-plugin-exporter-txt": "^0.16.24",
    "better-write-plugin-importer": "^0.16.24",
    "better-write-plugin-theme": "^0.16.24",
    "better-write-types": "^0.16.24",
    "date-fns": "2.28.0",
    "destr": "1.1.1",
    "drauu": "0.3.0",
    "file-saver": "2.0.5",
    "floating-vue": "2.0.0-beta.20",
    "mitt": "3.0.0",
    "nprogress": "0.2.0",
    "pinia": "2.0.22",
