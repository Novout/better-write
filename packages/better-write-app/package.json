    "@vuelidate/validators": "2.0.0-alpha.31",
    "@vueuse/core": "9.2.0",
    "@vueuse/components": "9.2.0",
    "@vueuse/head": "0.7.12",
    "@vueuse/integrations": "9.2.0",
    "@vueuse/motion": "2.0.0-beta.12",
    "@vueuse/schema-org-vite": "1.1.0-beta.4",
    "@vueuse/shared": "9.2.0",
    "better-write-contenteditable-ast": "^0.16.24",
    "better-write-extension": "^0.16.24",
    "better-write-google-fonts-api": "^0.16.24",
