{
  "name": "better-write-app",
  "version": "0.12.8",
  "repository": "https://github.com/Novout/better-write",
  "author": {
    "name": "Giovane Cardoso",
    "email": "novout@hotmail.com",
    "url": "https://github.com/Novout"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "description": "A editor for creative writers.",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "rimraf ./dist && yarn build && vite preview",
    "format": "prettier test/**/*.ts src/**/*.{vue,css,ts} --write",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src && yarn format",
    "extract": "vue-i18n-extract report --vueFiles './src/**/*.?(js|vue)' --languageFiles './src/lang/*.?(json|yml|yaml|js)'"
  },
  "dependencies": {
    "@headlessui/vue": "1.4.3",
    "@supabase/supabase-js": "1.29.1",
    "@vitest/ui": "0.2.4",
    "@vuelidate/core": "2.0.0-alpha.32",
    "@vuelidate/validators": "2.0.0-alpha.25",
    "@vueuse/core": "7.5.5",
    "@vueuse/head": "0.7.4",
    "@vueuse/integrations": "7.5.5",
    "@vueuse/motion": "2.0.0-beta.9",
    "better-write-plugin-core": "^0.12.8",
    "better-write-plugin-docx": "^0.12.8",
    "better-write-plugin-logger": "^0.12.8",
    "better-write-plugin-pdf": "^0.12.8",
    "better-write-plugin-theme": "^0.12.8",
    "better-write-types": "^0.12.8",
    "date-fns": "2.27.0",
    "dropbox": "10.23.0",
    "file-saver": "2.0.5",
    "keyboardjs": "2.6.4",
    "mitt": "3.0.0",
    "pinia": "2.0.10",
    "v-tooltip": "4.0.0-beta.2",
    "vite-plugin-package-version": "1.0.2",
    "vue": "3.2.29",
    "vue-i18n": "9.1.9",
    "vue-router": "4.0.12",
    "vue-toastification": "2.0.0-rc.5",
    "vue-typical": "2.1.0",
    "windicss": "3.4.3"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "3.2.1",
    "@types/file-saver": "2.0.5",
    "@types/jsdom": "16.2.14",
    "@typescript-eslint/eslint-plugin": "5.10.1",
    "@typescript-eslint/parser": "5.10.1",
    "@vitejs/plugin-vue": "2.1.0",
    "@vue/compiler-sfc": "3.2.29",
    "@vue/eslint-config-typescript": "9.1.0",
    "@vue/test-utils": "2.0.0-rc.18",
    "c8": "7.11.0",
    "cross-env": "7.0.3",
    "eslint": "8.4.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-vue": "8.2.0",
    "happy-dom": "latest",
    "prettier": "2.5.1",
    "rimraf": "3.0.2",
    "standard-version": "9.3.2",
    "typescript": "4.5.2",
    "unplugin-vue-components": "0.17.15",
    "vite": "2.7.13",
    "vite-plugin-fonts": "0.2.2",
    "vite-plugin-optimize-persist": "0.1.2",
    "vite-plugin-package-config": "0.1.1",
    "vite-plugin-pwa": "0.11.13",
    "vite-plugin-windicss": "1.6.3",
    "vitest": "latest",
    "vue-i18n-extract": "2.0.4",
    "vue-tsc": "0.29.8",
    "wait-on": "6.0.0"
  },
  "vite": {
    "optimizeDeps": {
      "include": [
        "@headlessui/vue",
        "@supabase/supabase-js",
        "@vue/reactivity",
        "@vueuse/core",
        "@vueuse/head",
        "@vueuse/integrations",
        "@vueuse/motion",
        "date-fns",
        "date-fns/locale",
        "docx",
        "dropbox",
        "es5-ext/global",
        "file-saver",
        "jiti",
        "keyboardjs",
        "mitt",
        "node-fetch",
        "pdfmake/build/pdfmake",
        "pdfmake/build/vfs_fonts",
        "pinia",
        "v-tooltip",
        "vue",
        "vue-i18n",
        "vue-router",
        "vue-toastification",
        "vue-typical"
      ],
      "exclude": [
        "vue-demi"
      ]
    }
  }
}
