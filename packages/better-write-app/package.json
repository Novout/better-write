{
  "name": "better-write-app",
  "version": "0.16.24",
  "repository": "https://github.com/Novout/better-write",
  "author": {
    "name": "Giovane Cardoso",
    "email": "novout@hotmail.com",
    "url": "https://github.com/Novout"
  },
  "publishConfig": {
    "access": "public"
  },
  "description": "A editor for creative writers.",
  "scripts": {
